        ADDR : 0x000000F0
        SIZE : 2
        BURST: INCR
        ID   : 10
        DATA : [0xF0,0xF1,0xF2,0xF3,
                0xF4,0xF5,0xF6,0xF7,
                0xF8,0xF9,0xFA,0xFB,
                0xFC,0xFD,0xFE,0xFF]
